extends layout

block content
	.container-fluid
		.row
			h1 All Tasks
			table#tasks_table(class='table table-striped table-hover table-condensed')
				thead
					tr
						th Task ID
						th Task Type
						th CMD
						th URL
						th POC
						th Min Time(ms)
						th Time(ms)
						th Submit Time
						th Completed
				tbody#tasks_tbody

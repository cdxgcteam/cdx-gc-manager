extends layout

block content
	.container-fluid
		.row
			.col-md-8
				h1 Malicious Tasks
				table#mal_table(class='table table-striped table-hover table-condensed')
					thead
						tr
							th Task ID
							th URL
							th POC
							th Min Time(ms)
							th Time(ms)
							th Submit Time
							th Completed
					tbody#mal_tbody
						tr
							td 2fd4e1c67a2d28fced849ee1bb76e7391b93eb12
							td
								a(href='http://www.google.com',data-toggle="tooltip",data-placement="right",title="Tooltip on right") Link
							td dyap
							td 500 ms
							td 34500 ms
							td 2014-02-04T02:22:32.076Z
							td
								span(class='badge') 20
						
			.col-md-4
				h1 Malicious Input
				form(role='form')
					.form-group
						label(for='inputURL') URL
						input#inputURL(type='text', class='form-control', placeholder='Enter or copy URL')
					.form-group
						label(for='inputPOC') POC
						input#inputPOC(type='text', class='form-control', placeholder='Enter POC')
					.form-group
						label(for='inputMinTime') Minimum Time
						input#inputMinTime(type='text', class='form-control', placeholder='Enter Minimum Time')
					button(type='submit',class='btn btn-default') Submit
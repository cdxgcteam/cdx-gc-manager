extends layout

block content
	.container-fluid
		.row
			.col-md-2
				h2 Total Created
				h3
					span#totalcreated 0
			.col-md-2
				h2 Total Recieved
				h3
					span#totalrecieved 0
			.col-md-2
				h2 Total Generated
				h3
					span#totalgen 0				
			.col-md-2
				h2 Total Malicious
				h3
					span#totalmal 0
			.col-md-2
				h2 Mal Queue Size
				h3
					span#totalmalqueue 0
			.col-md-2
				h2 CMD Queue Size
				h3
					span#totalcmdqueue 0
		.row
			h1 Malicious Tasks Executed
			table#mal_table(class='table table-striped table-hover table-condensed')
				thead
					tr
						th Task ID
						th Task Type
						th CMD
						th URL
						th POC
						th Min Time(ms)
						th Time(ms)
						th Submit Time
						th Completed
				tbody#mal_tbody

